      "safari/4.0..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2"
    ]
  },
  "engines": {
    "node": ">= 0.4"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
